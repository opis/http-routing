{
    "name": "opis/http-routing",
    "description": "Opis HTTP Routing is a library that can be used to route all types of HTTP request, providing a full range of features, like path filters, domain filters, user defined filters, access filters, custom error handlers for HTTP errors and much more. ",
    "keywords": ["routing", "http", "serializable"],
    "homepage": "http://opis.io",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Marius Sarca",
            "email": "marius.sarca@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "opis/routing": "5.0.*@dev",
        "opis/http": "3.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Opis\\HttpRouting\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opis\\HttpRouting\\Test\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "5.0.x-dev"
        }
    }
}
