{
    "name": "opis/http-routing",
    "description": "Opis HTTP Routing is a library that can be used to route all types of HTTP requests",
    "keywords": ["routing", "http", "http-message"],
    "homepage": "https://opis.io/http-routing",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Marius Sarca",
            "email": "marius.sarca@gmail.com"
        },
        {
            "name": "Sorin Sarca",
            "email": "sarca_sorin@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "opis/routing": "^5.0",
        "opis/http": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Opis\\HttpRouting\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opis\\HttpRouting\\Test\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "5.0.x-dev"
        }
    }
}
